{
  "name": "whatispill",
  "version": "2.1.0",
  "repository": "https://github.com/KNUT-Capstone-Design-team-1/wip-application-v2",
  "scripts": {
    "android": "react-native run-android --mode=productdebug -- --reset-cache",
    "ios": "react-native run-ios --scheme whatispill_production -- --reset-cache",
    "lint": "eslint .",
    "start": "react-native start -- --reset-cache",
    "pod": "cd ios && pod deintegrate && pod install && cd ..",
    "podstart": "cd ios && pod deintegrate && pod install && cd .. && react-native start -- --reset-cache",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew bundleRelease && cd ..",
    "ipa": "xcodebuild -workspace ios/whatispill.xcworkspace -scheme whatispill_production -sdk iphoneos -configuration Release -archivePath build/whatispill.xcarchive archive && xcodebuild -exportArchive -archivePath build/whatispill.xcarchive -exportOptionsPlist ios/exportOptions.plist -exportPath build/",
    "reset:android": "bash ./reset_build_cache.sh android",
    "reset:ios": "bash ./reset_build_cache.sh ios"
  },
